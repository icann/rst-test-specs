#exec sed 's/^/  /' ChangeLog.yaml

Test-Plans:
#exec gpp -x inc/plans.yaml.in | sed 's/^/  /'

Test-Suites:
#exec gpp -x inc/suites.yaml.in | sed 's/^/  /'

